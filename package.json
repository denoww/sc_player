{
  "name": "sc_player",
  "version": "2.3.0",
  "description": "Player",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": "12.22.0"
  },
  "scripts": {
    "start": "npm run start-node & npm run start-electron",
    "start-node": "node server.js",
    "start-electron": "electron --disable-gpu --no-sandbox .",

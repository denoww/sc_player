    "compile-coffee": "coffee -wc app/assets/**"
  },
  "dependencies": {
    "@sentry/browser": "6.2.5",
    "@sentry/electron": "2.4.0",
    "@sentry/integrations": "6.2.5",
    "@sentry/node": "6.2.5",
    "coffeescript": "2.5.1",
    "electron": "9.2.1",
    "electron-context-menu": "2.3.0",
    "electron-rebuild": "2.0.0",
    "express": "4.17.1",
    "jimp": "0.14.0",
    "md5": "2.3.0",
    "moment": "2.27.0",
    "node-env-file": "0.1.8",
    "nodemon": "2.0.4",
    "qrcode": "1.4.4",
    "rss-parser": "3.9.0",
    "sc-node-tools": "1.3.5",
    "sharp": "0.25.4",
    "shelljs": "0.8.4",
    "url-exists": "1.0.3"
